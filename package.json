{
  "name": "website-text-translator",
  "version": "1.0.1",
  "description": "Easy command line interface to automatically translate the text in your HTML file(s) to multiple languages and store the translation data in JSON files. Auto-detect changes and straightforward integration into your workflow. Documentation includes code snippets to integrate the translation data into your website project using vanilla JavaScript. Usage of a third party translation services, e.g. Google Translate API, is recommended but optional.",
  "bin": {
    "wtt": "bin/index.js"
  },
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "wtt": "node ./bin/index.js",
    "wtt-config": "node ./bin/index.js config",
    "wtt-strip": "node ./bin/index.js strip",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jorishr/website-text-translator.git"
  },
  "keywords": [
    "translation",
    "translator",
    "html",
    "json",
    "language",
    "website"
  ],
  "author": {
    "name": "Joris Raymaekers",
    "email": "joris@liondigits.com",
    "url": "https://liondigits.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jorishr/website-text-translator/issues"
  },
  "homepage": "https://github.com/jorishr/website-text-translator#readme",
  "dependencies": {
    "@google-cloud/translate": "^7.2.0",
    "commander": "^10.0.0",
    "deepmerge": "^4.3.0",
    "node-html-parser": "^6.1.5"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^29.7.0"
  }
}

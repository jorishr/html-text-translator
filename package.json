{
  "name": "website-text-translator",
  "version": "1.0.0",
  "description": "Automatically translate HTML files to multiple languages with Google Translate API, detect changes and store translations in JSON files.",
  "bin": {
    "wtt": "bin/index.js"
  },
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "wtt": "node ./bin/index.js",
    "wtt-config": "node ./bin/index.js config",
    "wtt-strip": "node ./bin/index.js strip",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jorishr/website-text-translator.git"
  },
  "keywords": [
    "translation",
    "translator",
    "html",
    "json",
    "language",
    "website"
  ],
  "author": "Joris Raymaekers <joris@liondigits.com> (https://liondigits.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jorishr/website-text-translator/issues"
  },
  "homepage": "https://github.com/jorishr/website-text-translator#readme",
  "dependencies": {
    "@google-cloud/translate": "^7.2.0",
    "commander": "^10.0.0",
    "deepmerge": "^4.3.0",
    "node-html-parser": "^6.1.5"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^29.7.0"
  }
}
